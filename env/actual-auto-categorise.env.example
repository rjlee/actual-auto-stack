# Copy to actual-auto-categorise.env and fill values specific to actual-auto-categorise

# Optional: override shared Actual connection (otherwise inherited from stack/.env)
ACTUAL_SERVER_URL=
ACTUAL_PASSWORD=
ACTUAL_SYNC_ID=
ACTUAL_BUDGET_ENCRYPTION_PASSWORD=

# Web UI / server
HTTP_PORT=3000
LOG_LEVEL=info

# Shared auth gateway configuration (single gateway reused by the stack)
CATEGORISE_LOGIN_NAME=Actual Auto Categorise
CATEGORISE_AUTH_COOKIE_NAME=categorise-auth
# Optionally override the auth proxy image (defaults to GHCR latest)
# AUTH_FORWARD_IMAGE=ghcr.io/rjlee/actual-auto-auth:latest

# Scheduling
DISABLE_CRON_SCHEDULING=false
CLASSIFY_CRON=0 * * * *
CLASSIFY_CRON_TIMEZONE=UTC
TRAIN_CRON=30 6 * * 1
TRAIN_CRON_TIMEZONE=UTC

# Classifier backend: ml (Embed+KNN) or tf (TensorFlow.js)
CLASSIFIER_TYPE=ml

# Data directories
# Budget cache lives under /app/data/budget (set by stack). Override only if needed.
# BUDGET_DIR=/app/data/budget

# Copy to actual-auto-reconcile.env and fill values specific to actual-auto-reconcile

# Optional: override shared Actual connection (otherwise inherited from stack/.env)
ACTUAL_SERVER_URL=
ACTUAL_PASSWORD=
ACTUAL_SYNC_ID=
ACTUAL_BUDGET_ENCRYPTION_PASSWORD=

# Scheduling / behavior
RECONCILE_DELAY_DAYS=5
DISABLE_CRON_SCHEDULING=false
RECONCILE_CRON=0 * * * *
RECONCILE_CRON_TIMEZONE=UTC
LOG_LEVEL=info

# Event-based triggers (optional)
ENABLE_EVENTS=false
EVENTS_URL=http://actual-events:4000/events
EVENTS_AUTH_TOKEN=

# Budget cache lives under /app/data/budget (set by stack). Override only if needed.
# BUDGET_DIR=/app/data/budget

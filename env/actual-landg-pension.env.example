# Copy to actual-landg-pension.env and fill values specific to actual-landg-pension

# Optional: override shared Actual connection (otherwise inherited from stack/.env)
ACTUAL_SERVER_URL=
ACTUAL_PASSWORD=
ACTUAL_SYNC_ID=
ACTUAL_BUDGET_ENCRYPTION_PASSWORD=

# Web UI / server
HTTP_PORT=3000
UI_AUTH_ENABLED=true
SESSION_SECRET=
LOG_LEVEL=info

# Legal & General credentials
LANDG_EMAIL=
LANDG_PASSWORD=
LANDG_COOKIES_FILE=/app/data/landg_cookies.json
LANDG_2FA_TIMEOUT=60

# Budget cache lives under /app/data/budget (set by stack). Override only if needed.
# BUDGET_DIR=/app/data/budget

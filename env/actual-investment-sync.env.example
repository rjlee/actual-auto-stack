# Copy to actual-investment-sync.env and fill values specific to actual-investment-sync

# Optional: override shared Actual connection (otherwise inherited from stack/.env)
ACTUAL_SERVER_URL=
ACTUAL_PASSWORD=
ACTUAL_SYNC_ID=
ACTUAL_BUDGET_ENCRYPTION_PASSWORD=

# Web UI / server
HTTP_PORT=
INVEST_SYNC_AUTH_COOKIE_NAME=actual-auth
INVEST_SYNC_LOGIN_NAME=actual-investment-sync
LOG_LEVEL=info

# Scheduling
SYNC_CRON=0 * * * *
SYNC_CRON_TIMEZONE=UTC

# Budget cache lives under /app/data/budget (set by stack). Override only if needed.
# BUDGET_DIR=/app/data/budget
